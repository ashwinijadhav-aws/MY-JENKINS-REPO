pipeline {
		agent any
		tools {
			maven "Maven_Local"
		}
		stages {
			stage('Checkout') {
				steps {
						   
						checkout scmGit(
										 branches: [[name: '*/main']], 
										 extensions: [], 
										 userRemoteConfigs: [[credentialsId: 'GIT_UTHENTICATION_USING_SSH', url: 'https://github.com/ashwinijadhav-aws/MY-JENKINS-REPO']]
										)
						 sh "ls -lat"
				  }
				}
			}
			
		stage('Build'){
		
				steps {
					sh "mvn clean install -f JENKINS-TOMCAT-PROJECT/java-tomcat-sample/pom.xml"
				}
			} 

			
		}